<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.song.mapper.BaseCityMapper">

	<select id="selectList" parameterType="com.song.entity.BaseCity" resultType="com.song.entity.BaseCity">
		select * from base_city 
		where 0 = 0
		<if test="parentId !=null and parentId !=''">
			and parent_id = #{parentId}
		</if>
		<if test="cityName !=null and cityName !=''">
			and city_name  like concat('%',#{cityName},'%')
		</if>
		<if test="cityLevel !=null ">
			and city_level = #{cityLevel}
		</if>
	</select>
	

</mapper>