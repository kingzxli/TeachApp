<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.song.mapper.ViolationsMapper">

	<insert id="add" parameterType="com.song.pojo.Violations">
		insert into violations(content,tid,addtime) values (#{content},#{tid},#{addtime})
	</insert>
	
	<select id="select" resultType="com.song.pojo.Violations">
		select * from violations where tid=#{tid}
	</select>
	
	<select id="selectByTime" resultType="com.song.pojo.Violations">
		select * from violations where addtime = #{times}
	</select>
	
</mapper>