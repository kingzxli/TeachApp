<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.song.mapper.ExamMapper">


	<insert id="insert" parameterType="com.song.pojo.Exam">
		insert into exam(addtime,types,project,grade,ordernum,etimes)values(#{addtime},#{types},#{project},#{grade},#{ordernum},#{etimes})
	</insert>
	
	<update id="update" parameterType="com.song.pojo.Exam">
		update exam set
		<trim suffixOverrides=",">
			<if test="addtime !=null">
				addtime=#{addtime},
			</if>
			<if test="types !=null">
				types=#{types},
			</if>
			<if test="project !=null">
				project=#{project},
			</if>
			<if test="grade !=null">
				grade=#{grade}
			</if>
		</trim>
		where eid=#{eid}
	</update>
	
	<delete id="delete" parameterType="java.lang.String">
		delete from exam where ordernum=#{ordernum}
	</delete>
	
	<select id="selectById" parameterType="java.lang.String" resultType="com.song.pojo.Exam">
		select * from exam where ordernum=#{ordernum}
	</select>
	
</mapper>