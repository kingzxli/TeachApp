<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.song.mapper.DrawMapper">

	<insert id="add" parameterType="com.song.pojo.Draw">
		insert into draw(openid,addtime,money,zhifubao,zname) values (#{openid},#{addtime},#{money},#{zhifubao},#{zname})
	</insert>
	
	<select id="selectByOpenid" parameterType="java.lang.String" resultType="com.song.pojo.Draw">
		select * from draw where openid=#{openid}
	</select>
	
</mapper>