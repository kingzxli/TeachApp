<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.song.mapper.UsersMapper">


	<select id="Login" parameterType="Users" resultType="Users">
		select * from users where username = #{username} and password = #{password}
	</select>
	
	<insert id="add" parameterType="Users">
		insert into users(username,password,sex,year,month,date,encrypted) values (#{username},#{password},#{sex},#{year},#{month},#{date},#{encrypted})
	</insert>


</mapper>